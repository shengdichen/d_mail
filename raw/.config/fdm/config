# setup {{{
# do NOT add any |Received| tag
set no-received

# account {{{
account "acc_hold"
    maildirs {
        "/home/main/.local/share/mail/all/.hold"
    }
account "acc_raw_inbox"
    maildirs {
        "/home/main/.local/share/mail/raw/xyz/.INBOX"
