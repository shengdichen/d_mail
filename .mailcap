text/html; firefox-developer-edition --private-window %s 1>/dev/null 2>&1 &;
text/html; w3m -T text/html -dump | cat -n; copiousoutput;

text/*; nvim -R;
text/*; fold -w 119 -s | cat -n; copiousoutput;

application/pdf; zathura %s 1>/dev/null 2>&1 &;

image/*; imv %s & 1>/dev/null 2>&1;
audio/*; mpv %s & 1>/dev/null 2>&1;
video/*; mpv %s & 1>/dev/null 2>&1;
