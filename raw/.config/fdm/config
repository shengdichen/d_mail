# action {{{
action "delete" {
    drop
}
action "trash" {
    maildir "/home/main/.local/share/mail/all/.trash"
}
action "hold" {
    maildir "/home/main/.local/share/mail/all/.hold"
}
action "keep" {
    keep
}
action "print" {
    stdout
}
# }}}
# }}}

match
    accounts {
        "acc_hold"
    }

    actions {
        "keep"
    }

