defaults
auth on
tls on
tls_starttls on

# outlook {{{
account outlook
host smtp-mail.outlook.com
port 587
user shengdi@outlook.de
from shengdi@outlook.de
passwordeval gpg -dq ~/mail/pw/outlook.gpg
